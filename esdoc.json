{
  "source": "./src",
  "destination": "./docs",
  "includes": ["\\.js$"],
  "excludes": [
    "\\.config\\.js$",
    "\\.test\\.js$"
  ],
  "plugins": [{
    "name": "esdoc-standard-plugin",
    "option": {
      "lint": {
        "enable": true
      },
      "coverage": {
        "enable": true
      },
      "accessor": {
        "access": [
          "public"
        ],
        "autoPrivate": true
      },
      "undocumentIdentifier": {
        "enable": false
      },
      "unexportedIdentifier": {
        "enable": false
      },
      "typeInference": {
        "enable": true
      },
      "manual": {
        "index": "./README.md",
        "globalIndex": true,
        "asset": "./manual",
        "files": [
          "./README.md",
          "./manual/json/query.md"
        ]
      }
    }
  }]
}