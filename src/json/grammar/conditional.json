{
  "grammar": {
    "Ternary": [
      ["Condition", "?", "Assignable", ":", "Assignable"]
    ],

    "Condition": [
      ["Condition", "&&", "Logical"],
      ["Condition", "||", "Logical"],
      ["Logical"]
    ],

    "Logical": [
      ["Comparable"],
      ["Compare"]
    ],

    "Comparable": [
      ["Object"],
      ["Array"],
      ["Scalar"],
      ["Boolean"],
      ["null"],
      ["!", "Comparable"]
    ],

    "Compare": [
      ["Comparable", "===", "Comparable"],
      ["Comparable", "!==", "Comparable"],
      ["Comparable", "==", "Comparable"],
      ["Comparable", "!=", "Comparable"],
      ["Scalar", "=~", "string"],
      ["Scalar", "=~", "regex"],
      ["Scalar", ">", "Scalar"],
      ["Scalar", ">=", "Scalar"],
      ["Scalar", "<", "Scalar"],
      ["Scalar", "<=", "Scalar"]
    ]
  }
}