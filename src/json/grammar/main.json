{
  "root": "Query",
  "grammar": {
    "Query": [
      ["Stmt", "$"],
      ["Def", ";", "Stmt", "$"]
    ],

    "Def": [
      ["Def", ";", "Var"],
      ["Var"]
    ],

    "Var": [
      ["ident", "from", "Assignable"]
    ],

    "Stmt": [
      ["Stmt", ";", "Expr"],
      ["Stmt", ";"],
      ["Expr"]
    ],

    "Expr": [
      ["Assignable"],
      ["Unset"]
    ],

    "Assignable": [
      ["Ternary"],
      ["Condition"],
      ["Assign"]
    ],

    "Assign": [
      ["Path", "=", "Assignable"]
    ],

    "Unset": [
      ["delete", "UnsetPaths"]
    ],

    "UnsetPaths": [
      ["UnsetPaths", ",", "Path"],
      ["Path"]
    ]
  }
}
