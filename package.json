{
  "name": "@dikolab/basic",
  "version": "0.1.0",
  "description": "Basic Javascript helpers",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "test:rollup": "rollup --config rollup.config.test.js",
    "test:mocha": "mocha dist/test/mocha-feed.js",
    "pretest": "clear",
    "test": "run-s build:parser test:rollup test:mocha",
    "build:parser-states": "node lib/slr/generate-slr-states -o -c slr-config.json",
    "build:compiler-reference": "node lib/slr/generate-build-reference slr-build-reference.json src/json/build-reference.json",
    "build:utf-helper": "node lib/utf/generate-helper src/string/utf-constants.json",
    "build:parser": "run-s build:parser-states build:compiler-reference build:utf-helper",
    "build:source": "rollup --config",
    "build": "run-s clean:output build:parser build:source",
    "prepare": "run-s build",
    "lint": "eslint 'src/**/*.js'",
    "tdd": "nodemon --ignore 'src/json/parse-states.json' --ignore 'src/json/build-reference.json' --ignore 'src/string/utf-constants.json' --watch src --watch slr-build-reference.json --exec 'npm run test'",
    "tdb:parser": "nodemon --ignore 'src/json/parse-states.json' --ignore 'src/json/build-reference.json' --ignore 'src/string/utf-constants.json' --watch 'src/json/grammar/*.json' --exec 'npm run build:parser'",
    "jsdoc:build": "jsdoc -c jsdoc.json src",
    "doc:build": "esdoc -c esdoc.json",
    "docdoc:build": "documentation build src/** -f html -o docs",
    "doc": "run-s clean:doc doc:build",
    "doc-dev:build": "nodemon -V --watch src --watch esdoc.json --ignore 'src/**/*.json' --watch README.md --watch manual --ext js,md,json --exec 'npm run doc:build'",
    "doc-dev:serve": "browser-sync start --config browsersync.doc.js",
    "predoc-dev": "npm run clean:doc",
    "doc-dev": "run-p doc-dev:build doc-dev:serve",
    "clean:doc": "del 'docs/*'",
    "clean:output": "del dist/umd dist/system dist/cjs dist/esm dist/test",
    "clean": "run-p clean:doc clean:output"
  },
  "files": [
    "dist/cjs/*",
    "dist/esm/*",
    "dist/umd/*",
    "dist/system/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diko316/dikolab-basic.git"
  },
  "keywords": [
    "NodeJS",
    "Javascript",
    "core",
    "basic",
    "dikolab",
    "Tech Slave"
  ],
  "author": "Diko Tech Slave",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/diko316/dikolab-basic/issues"
  },
  "homepage": "https://www.dikoconsunji.com/javascript/dikolab-basic",
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-multi-entry": "^3.0.1",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-strip": "^1.3.3",
    "browser-sync": "^2.26.7",
    "chai": "^4.2.0",
    "clear-cli": "^1.0.1",
    "del-cli": "^3.0.1",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-strict": "^14.0.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^28.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.19.0",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-terser": "^6.1.0",
    "unexpected": "^11.14.0"
  }
}
