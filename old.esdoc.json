{
  "source": "./src",
  "destination": "./docs",
  "includes": ["\\.js$"],
  "excludes": [
    "\\.config\\.js$",
    "\\.test\\.js$"
  ],
  "plugins": [{
    "name": "esdoc-standard-plugin",
    "option": {
      "lint": {
        "enable": true
      },
      "coverage": {
        "enable": true
      },
      "accessor": {
        "access": [
          "public",
          "protected",
          "private"
        ],
        "autoPrivate": true
      },
      "undocumentIdentifier": {
        "enable": false
      },
      "unexportedIdentifier": {
        "enable": false
      },
      "typeInference": {
        "enable": true
      },
      "brand": {
        "logo": "./logo.png",
        "title": "@dikolab/basic",
        "description": "Basic helpers",
        "repository": "https://github.com/diko316/dikotech-basic",
        "site": "http://my-library.org",
        "author": "https://twitter.com/foo",
        "image": "http://my-library.org/logo.png"
      }
    }
  }]
}